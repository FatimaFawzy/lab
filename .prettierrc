{
     "printWidth": 120,
     "trailingComma": "es5",
     "tabWidth": 5,
     "semi": true,
     "singleQuote": false,
     "bracketSpacing": true,
     "bracketSameLine": true,
     "jsxBracketSameLine": true,
     "arrowParens": "always",
     "endOfLine": "auto",
     "jsxSingleQuote": true,
     "proseWrap": "preserve",
     "quoteProps": "as-needed",
     "useTabs": false,
     "htmlWhitespaceSensitivity": "ignore",
     "vueIndentScriptAndStyle": true
}
